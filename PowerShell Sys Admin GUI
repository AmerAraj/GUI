	$buttonInstallSoftwareExamp.add_Click($buttonInstallSoftwareExamp_Click)
	#
	# labelListOfClientPCs
	#
	$labelListOfClientPCs.AutoSize = $True
	$labelListOfClientPCs.Font = 'Microsoft Sans Serif, 8.25pt'
	$labelListOfClientPCs.Location = '3, 8'
	$labelListOfClientPCs.Name = 'labelListOfClientPCs'
	$labelListOfClientPCs.Size = '91, 17'
	$labelListOfClientPCs.TabIndex = 44
	$labelListOfClientPCs.Text = 'List of Client PCs'
	$labelListOfClientPCs.TextAlign = 'MiddleCenter'
	$labelListOfClientPCs.UseCompatibleTextRendering = $True
	$labelListOfClientPCs.add_Click($labelListOfClientPCs_Click)
	$tabpage5.ResumeLayout()
	$tabpage4.ResumeLayout()
	$tabpage3.ResumeLayout()
	$tabpage2.ResumeLayout()
	$tabpage1.ResumeLayout()
	$tabcontrol1.ResumeLayout()
	$form1.ResumeLayout()
	#endregion Generated Form Code

	#----------------------------------------------

	#Save the initial state of the form
	$InitialFormWindowState = $form1.WindowState
	#Init the OnLoad event to correct the initial state of the form
	$form1.add_Load($Form_StateCorrection_Load)
	#Clean up the control events
	$form1.add_FormClosed($Form_Cleanup_FormClosed)
	#Show the Form
	return $form1.ShowDialog()

} #End Function

