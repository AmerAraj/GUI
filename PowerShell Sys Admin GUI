	$Form_Cleanup_FormClosed=
	{
		#Remove all event handlers from the controls
		try
		{
			$button4.remove_Click($button4_Click)
			$buttonADUC.remove_Click($buttonADUC_Click)
			$buttonDeviceMgr.remove_Click($buttonDeviceMgr_Click)
			$buttonRegistry.remove_Click($buttonRegistry_Click)
			$buttonMMC.remove_Click($buttonMMC_Click)
			$TaskMgr.remove_Click($TaskMgr_Click)
			$button7.remove_Click($button7_Click)
			$buttonServer8.remove_Click($buttonServer8_Click)
			$buttonServer3.remove_Click($buttonServer3_Click)
			$buttonServer4.remove_Click($buttonServer4_Click)
			$buttonServer5.remove_Click($buttonServer5_Click)
